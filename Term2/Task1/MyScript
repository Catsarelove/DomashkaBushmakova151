# !/bin/bash
make -f Makefile.txt
exec 6>&1
exec 7<&0
for ((n=1; n<=10; n++))
do
exec 0<test/$[n]_test_in.txt
exec 1>test/$[n]_test_out.txt
./myProg1
exec 1>&6
exec 0<&7
if cmp -s test/$[n]_test_res.txt test/$[n]_test_out.txt ; then
        echo $[n] тест пройден
        else echo $[n] тест не пройден
fi
done
